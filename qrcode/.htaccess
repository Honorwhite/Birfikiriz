RewriteEngine On
RewriteBase /qrcode/

# If the request is a real file or directory, serve it
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]

# Force everything else to index.html
# We use /qrcode/index.html to be absolutely explicit
RewriteRule ^(.*)$ /qrcode/index.html [L]